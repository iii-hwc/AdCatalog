<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.ad.catalog"
    android:versionCode="1"
    android:versionName="1.0">
    <uses-sdk android:minSdkVersion="4"
        android:targetSdkVersion="12"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".AdCatalog"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Banners"/>
        <activity android:name=".Interstitials"/>
        <activity android:name=".GameLevels"/>
        <activity android:name=".VideoPreroll"/>
        <activity android:name=".PageSwipe"/>
        <activity android:name=".layouts.AdvancedLayouts"/>
        <activity android:name=".layouts.OpenGLViewExample"/>
        <activity android:name=".layouts.TabbedViewExample"/>
        <activity android:name=".layouts.ListViewExample"/>
        <activity android:name=".layouts.ScrollViewExample"/>
        <activity android:name="com.google.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"/>
    </application>
</manifest>
